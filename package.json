{
  "name": "meal-planner",
  "version": "0.0.1",
  "description": "A way to plan your meals and create grocery lists.",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "es6-promise": "^4.2.5",
    "express": "^4.16.4",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "mongodb": "^3.1.10",
    "mongoose": "^5.3.14",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-jest": "^22.4.4",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.27.2",
    "jest": "^22.4.4",
    "jest-environment-node": "^22.4.3",
    "npm-run-all": "^4.1.5",
    "superagent": "^3.8.3",
    "supertest": "^3.3.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "test": "jest --testURL=\"http://localhost\"",
    "seed": "node src/seed.js",
    "dev": "run-p dev-server build-watch ",
    "build-watch": "webpack --watch",
    "prestart": "npm run -s build",
    "start": "node server_dist/server.js",
    "dev-server": "nodemon src/server.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf server_dist",
    "build": "webpack && npm run clean && babel src -s -D -d server_dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeliaCodes/Meal-Planner.git"
  },
  "jest": {
    "testEnvironment": "./mongo-environment.js"
  },
  "keywords": [
    "food",
    "planning",
    "meals",
    "grocery",
    "shopping",
    "list"
  ],
  "engines": {
    "node": "8.9.0"
  },
  "author": "me@delia.codes",
